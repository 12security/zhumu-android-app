<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient android:startColor="#ccb5b6b5" android:endColor="#ccb5b6b5" android:angle="270" android:centerY="0.5" android:centerColor="#ccb5b6b5"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient android:startColor="#ff2da5ff" android:endColor="#ff2da5ff" android:angle="270" android:centerY="0.5" android:centerColor="#ff2da5ff"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#ff2da5ff" android:endColor="#ff2da5ff" android:angle="270" android:centerY="0.5" android:centerColor="#ff2da5ff"/>
            </shape>
        </clip>
    </item>
</layer-list>
