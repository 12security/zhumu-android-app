<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/zm_toolbar_big_radius"/>
            <gradient android:startColor="#ff8f00" android:endColor="#f38a03" android:angle="0"/>
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/zm_toolbar_big_radius"/>
            <gradient android:startColor="#fca83e" android:endColor="#fca83e" android:angle="0"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/zm_toolbar_big_radius"/>
            <gradient android:startColor="#e3e3ed" android:endColor="#d9d9e3" android:angle="0"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/zm_toolbar_big_radius"/>
            <gradient android:startColor="#fca83e" android:endColor="#ff8f00" android:angle="0"/>
        </shape>
    </item>
</selector>
