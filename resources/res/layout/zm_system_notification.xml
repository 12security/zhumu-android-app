<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <us.zoom.androidlib.widget.ZMIOSStyleTitlebarLayout android:id="@+id/panelTitleBar" android:layout_width="match_parent" android:layout_height="wrap_content" xmlns:ns1="http://schemas.android.com/apk/res-auto" ns1:zm_rightButton="@+id/btnCancel" xmlns:ns2="http://schemas.android.com/apk/res-auto" ns2:zm_title="@+id/txtTitle" style="@style/ZMTitleBar">
        <Button android:layout_gravity="right" android:id="@+id/btnCancel" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="10dp" android:text="@string/zm_btn_cancel" style="@style/ZMButton.TitleBar"/>
        <us.zoom.androidlib.widget.ZMDynTextSizeTextView android:ellipsize="end" android:gravity="center" android:id="@+id/txtTitle" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/zm_system_notification_title" android:singleLine="true" style="@style/ZMTextView.Title"/>
    </us.zoom.androidlib.widget.ZMIOSStyleTitlebarLayout>
    <com.zipow.videobox.view.mm.MMSystemNotificationListView android:id="@+id/systemNotificationListView" android:layout_width="match_parent" android:layout_height="match_parent"/>
    <LinearLayout android:layout_gravity="center_horizontal" android:orientation="vertical" android:id="@+id/panelNoItemMsg" android:paddingLeft="10dp" android:paddingRight="10dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="60dp">
        <include android:id="@+id/imgNoBuddy" layout="@layout/zm_image_no_system_notification"/>
        <TextView android:gravity="center" android:layout_gravity="center_horizontal" android:id="@+id/txtNoContactsMessage" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="30dp" android:text="@string/zm_system_notification_no_item" style="@style/ZMTextView.Large.OnLight"/>
    </LinearLayout>
</LinearLayout>
