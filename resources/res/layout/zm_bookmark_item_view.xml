<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:paddingLeft="10dp" android:paddingTop="5dp" android:paddingRight="10dp" android:paddingBottom="5dp" android:descendantFocusability="blocksDescendants" android:layout_width="match_parent" android:layout_height="wrap_content">
    <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="match_parent">
        <ImageView android:layout_gravity="center" android:id="@+id/ImageDelIcon" android:paddingLeft="2dp" android:paddingTop="2dp" android:paddingRight="2dp" android:paddingBottom="2dp" android:duplicateParentState="false" android:layout_width="25dp" android:layout_height="25dp" android:src="@drawable/zm_bookmark_remove_btn" android:contentDescription="@string/zm_description_btn_bookmark_remove"/>
        <ImageView android:layout_gravity="center" android:id="@+id/ImageBookmarkIcon" android:layout_width="30dp" android:layout_height="30dp" android:src="@drawable/zm_bookmark"/>
        <TextView android:ellipsize="end" android:gravity="center_vertical" android:layout_gravity="center_vertical" android:id="@+id/txtName" android:duplicateParentState="false" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_marginLeft="8dp" android:hint="Title" android:singleLine="true" android:layout_weight="1" style="@style/ZMTextView.Medium.OnLight"/>
        <ImageView android:layout_gravity="center" android:id="@+id/ImageEditIcon" android:paddingLeft="2dp" android:paddingTop="2dp" android:paddingRight="2dp" android:paddingBottom="2dp" android:duplicateParentState="false" android:layout_width="25dp" android:layout_height="25dp" android:src="@drawable/zm_next_arrow"/>
    </LinearLayout>
</LinearLayout>
