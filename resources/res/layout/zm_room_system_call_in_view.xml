            <TextView android:paddingLeft="@dimen/zm_setting_item_padding_left" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/zm_lbl_h323_ip_address" style="@style/ZMEditText.SettingsItem"/>
