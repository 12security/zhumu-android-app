<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:id="@+id/LinearLayout1" android:paddingLeft="10dp" android:paddingRight="10dp" android:layout_width="match_parent" android:layout_height="match_parent">
    <EditText android:id="@+id/editUserName" android:paddingTop="10dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:hint="@string/http_auth_dialog_username" android:ems="10">
        <requestFocus/>
    </EditText>
    <EditText android:id="@+id/editPassword" android:paddingTop="10dp" android:paddingBottom="20dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:hint="@string/http_auth_dialog_password" android:ems="10" android:inputType="textPassword"/>
</LinearLayout>
