<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item n1:state_focused="true" n1:state_pressed="true"
      xmlns:n1="http://schemas.android.com/apk/res/android">
        <shape>
            <solid n1:color="@color/zm_item_highlight_pressed" />
        </shape>
    </item>
    <item n2:state_focused="false" n2:state_pressed="true"
      xmlns:n2="http://schemas.android.com/apk/res/android">
        <shape>
            <solid n2:color="@color/zm_item_highlight_pressed" />
        </shape>
    </item>
    <item n3:state_focused="true"
      xmlns:n3="http://schemas.android.com/apk/res/android">
        <shape>
            <solid n3:color="@color/zm_item_highlight" />
        </shape>
    </item>
    <item n4:drawable="@color/zm_white"
      xmlns:n4="http://schemas.android.com/apk/res/android" />
</selector>