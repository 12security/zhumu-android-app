<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item n1:state_enabled="true" n1:state_pressed="true"
      xmlns:n1="http://schemas.android.com/apk/res/android">
        <shape n1:shape="rectangle">
            <corners n1:radius="3.5dip" />
            <stroke n1:width="1.0dip" n1:color="@color/zm_text_dim" />
            <solid n1:color="@color/zm_transparent" />
        </shape>
    </item>
    <item n2:state_enabled="true" n2:state_pressed="false"
      xmlns:n2="http://schemas.android.com/apk/res/android">
        <shape n2:shape="rectangle">
            <corners n2:radius="3.5dip" />
            <stroke n2:width="1.0dip" n2:color="@color/zm_text_dim" />
            <solid n2:color="@color/zm_transparent" />
        </shape>
    </item>
    <item n3:state_enabled="false"
      xmlns:n3="http://schemas.android.com/apk/res/android">
        <shape n3:shape="rectangle">
            <corners n3:radius="3.5dip" />
            <stroke n3:width="1.0dip" n3:color="@color/zm_text_disable" />
            <solid n3:color="@color/zm_transparent" />
        </shape>
    </item>
</selector>