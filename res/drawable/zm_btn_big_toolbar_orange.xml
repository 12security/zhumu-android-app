      xmlns:n1="http://schemas.android.com/apk/res/android">
        <shape n1:shape="rectangle">
            <corners n1:radius="@dimen/zm_toolbar_big_radius" />
            <gradient n1:startColor="#ffff8f00" n1:endColor="#fff38a03" n1:angle="0.0" />
        </shape>
    </item>
    <item n2:state_focused="true"
      xmlns:n2="http://schemas.android.com/apk/res/android">
