<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item n1:state_enabled="true" n1:state_pressed="false"
      xmlns:n1="http://schemas.android.com/apk/res/android">
        <shape n1:shape="rectangle">
            <solid n1:color="#00ffffff" />
        </shape>
    </item>
    <item n2:state_enabled="true" n2:state_pressed="true"
      xmlns:n2="http://schemas.android.com/apk/res/android">
        <shape n2:shape="rectangle">
            <solid n2:color="#33000000" />
            <corners n2:radius="8.0dip" />
        </shape>
    </item>
    <item n3:state_enabled="false"
      xmlns:n3="http://schemas.android.com/apk/res/android">
        <shape n3:shape="rectangle">
            <solid n3:color="#00ffffff" />
        </shape>
    </item>
</selector>