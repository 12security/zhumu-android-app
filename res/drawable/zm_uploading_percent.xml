<?xml version="1.0" encoding="utf-8"?>
<layer-list>
    <item n1:id="@android:id/background"
      xmlns:n1="http://schemas.android.com/apk/res/android">
        <shape>
            <solid n1:color="@color/zm_progress_bar_bg_ondark" />
        </shape>
    </item>
    <item n2:id="@android:id/secondaryProgress"
      xmlns:n2="http://schemas.android.com/apk/res/android">
        <clip>
            <shape>
                <solid n2:color="#ffff7700" />
            </shape>
        </clip>
    </item>
    <item n3:id="@android:id/progress"
      xmlns:n3="http://schemas.android.com/apk/res/android">
        <clip>
            <shape>
                <solid n3:color="@color/zm_white" />
            </shape>
        </clip>
    </item>
</layer-list>