<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item n1:state_focused="true"
      xmlns:n1="http://schemas.android.com/apk/res/android">
        <shape n1:shape="rectangle">
            <corners n1:radius="5.0dip" />
            <solid n1:color="#ff2dd7ff" />
        </shape>
    </item>
    <item n2:state_enabled="true" n2:state_pressed="false"
      xmlns:n2="http://schemas.android.com/apk/res/android">
        <shape n2:shape="rectangle">
            <corners n2:radius="5.0dip" />
            <solid n2:color="#ffffffff" />
        </shape>
    </item>
    <item n3:state_enabled="true" n3:state_pressed="true"
      xmlns:n3="http://schemas.android.com/apk/res/android">
        <shape n3:shape="rectangle">
            <corners n3:radius="5.0dip" />
            <solid n3:color="#fff89c08" />
        </shape>
    </item>
    <item n4:state_enabled="false"
      xmlns:n4="http://schemas.android.com/apk/res/android">
        <shape n4:shape="rectangle">
            <corners n4:radius="5.0dip" />
            <solid n4:color="#ffffffff" />
        </shape>
    </item>
</selector>